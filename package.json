{
  "name": "facebook-koolaid",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "migrations": "sequelize db:migrate",
    "lint": "eslint .",
    "flow": "flow",
    "validate": "npm run lint && npm run flow",
    "start": "nodemon --exec 'env NODE_ENV=development babel-node.js ./server/webpackDevServer.js'",
    "debug": "nodemon --exec 'env NODE_ENV=development babel-node --debug ./server/webpackDevServer.js'",
    "debug-brk": "nodemon --exec 'env NODE_ENV=development babel-node.js --debug --debug-brk ./server/webpackDevServer.js'",
    "build": "npm run validate && rm -rf ./dist && env NODE_ENV=production node webpack -p --config ./webpack.server.config.js && env NODE_ENV=production node webpack.js -p --config ./webpack.client.config.js"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.9.1",
    "bcryptjs": "^2.3.0",
    "body-parser": "~1.13.2",
    "cli-color": "^1.1.0",
    "config": "^1.19.0",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "express-graphql": "^0.5.3",
    "express-session": "^1.13.0",
    "file-loader": "^0.9.0",
    "graphql": "^0.6.1",
    "graphql-relay": "^0.4.2",
    "html-webpack-plugin": "^2.22.0",
    "jsonfile": "^2.3.0",
    "morgan": "~1.6.1",
    "nodemailer": "^2.4.2",
    "nodemailer-ses-transport": "^1.3.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^4.5.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-router": "^2.6.1",
    "sequelize": "^3.19.3",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "source-map-support": "^0.4.2",
    "tracer": "^0.8.3",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-tcomb": "^0.3.11",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babili-webpack-plugin": "0.0.2",
    "copy-webpack-plugin": "^3.0.1",
    "eslint": "^3.2.2",
    "eslint-plugin-flowtype": "^2.11.4",
    "eslint-plugin-react": "^6.0.0",
    "flow-bin": "^0.31.1",
    "nodemon": "^1.9.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.10",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-livereload-plugin": "^0.8.1"
  }
}
